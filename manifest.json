{
    "manifest_version": 3,
    "name": "xxx",
    "version": "0.2.0",
    "permissions": [
        "storage",
        "webRequest",
        "contextMenus",
        "tabs",
        "activeTab",
        "scripting"
    ],
    "background": {
        "service_worker": "service-worker.js"
    },
    
    
    "content_scripts": [
        {
            "matches": [   
           "https://*/*",
            "http://*/*"
            ],
            
            
            "js": [
                "scripts/content-script.js"
            ]
            
        }
    ]
}